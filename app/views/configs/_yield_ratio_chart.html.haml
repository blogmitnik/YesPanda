:javascript
  jQuery(function() {
    $('ul.nav a').on('shown.bs.tab', function (e) {
      accu_donut_graph.redraw();
      daily_donut_graph.redraw();
      fp_accu_donut_graph.redraw();
      fp_daily_donut_graph.redraw();
      accu_graph.redraw();
      daily_graph.redraw();
      fp_accu_graph.redraw();
      fp_daily_graph.redraw();
      $('svg').css({ width: '100%' });
    });
    var accu_donut_graph = Morris.Donut({
      resize: true,
      element: 'accu-yield-config-ratio',
      data: $('#accu-yield-config-ratio').data('reports'),
      backgroundColor: '#ddd',
      labelColor: '#666',
      colors: ['#afd8f8', '#1fbba6', '#f8aa33', '#cb4b4b', '#6d6d6d'],
      formatter: function (x, data) { return data.percent + '% (' + x + ')'; }
    });
    var daily_donut_graph = Morris.Donut({
      resize: true,
      element: 'daily-yield-config-ratio',
      data: $('#daily-yield-config-ratio').data('reports'),
      backgroundColor: '#ddd',
      labelColor: '#666',
      colors: ['#afd8f8', '#4da74d', '#ffc652', '#ff7070', '#848484'],
      formatter: function (x, data) { return data.percent + '% (' + x + ')'; }
    });
    var fp_accu_donut_graph = Morris.Donut({
      resize: true,
      element: 'fp-accu-yield-config-ratio',
      data: $('#fp-accu-yield-config-ratio').data('reports'),
      backgroundColor: '#ddd',
      labelColor: '#666',
      colors: ['#afd8f8', '#1fbba6', '#f8aa33', '#cb4b4b', '#6d6d6d'],
      formatter: function (x, data) { return data.percent + '% (' + x + ')'; }
    });
    var fp_daily_donut_graph = Morris.Donut({
      resize: true,
      element: 'fp-daily-yield-config-ratio',
      data: $('#fp-daily-yield-config-ratio').data('reports'),
      backgroundColor: '#ddd',
      labelColor: '#666',
      colors: ['#afd8f8', '#4da74d', '#ffc652', '#ff7070', '#848484'],
      formatter: function (x, data) { return data.percent + '% (' + x + ')'; }
    });
    var accu_graph = Morris.Bar({
      element: 'worst-accu-yield-data',
      smooth: true,
      continuousLine: true,
      behaveLikeLine: true,
      barColors: function(row, series, type) {
        if (type === 'bar') {
          if (row.y > 100) {
            return '#afd8f8';
          } else if (row.y >= 99.51 && row.y <= 100) {
            return '#1fbba6';
          } else if (row.y >= 90.00 && row.y <= 99.50) {
            return '#f8aa33';
          } else if (row.y >= 0 && row.y <= 89.99) {
            return '#cb4b4b';
          }
        } else {
          return '#000';
        }
      },
      lineWidth: 4,
      fillOpacity: 0.8,
      resize: true,
      hideHover: 'auto',
      continuousLine: true,
      data: $('#worst-accu-yield-data').data('reports'),
      goals: [90, 95.5, 100],
      goalLineColors: ['#e74c3c', '#e67e22', '#2ecc71'],
      xkey: 'published_at',
      ykeys: ['worst_accu_yield', 'worse_accu_yield', 'bad_accu_yield', 'bad2_accu_yield', 'bad3_accu_yield'],
      labels: ['worst_accu_config', 'worse_accu_config', 'bad_accu_config', 'bad2_accu_config', 'bad3_accu_config'],
      "hoverCallback":  function(index, options, content){
        var row = options.data[index]
        datetime = '<p><b>' + row.published_at + '</b></p>'
        var worst_accu_yield = row.worst_accu_yield == null? 'No Input' : row.worst_accu_yield + options.postUnits;
        var worse_accu_yield = row.worse_accu_yield == null? 'No Input' : row.worse_accu_yield + options.postUnits;
        var bad_accu_yield = row.bad_accu_yield == null? 'No Input' : row.bad_accu_yield + '%';
        var bad2_accu_yield = row.bad2_accu_yield == null? 'No Input' : row.bad2_accu_yield + '%';
        var bad3_accu_yield = row.bad3_accu_yield == null? 'No Input' : row.bad3_accu_yield + '%';

        station1 = '<div style="color: #555;"><strong>Most Worst: </strong>' + row.worst_accu_config + ' (' + worst_accu_yield + ')</div>'
        station2 = '<div style="color: #555;"><strong>Worst: </strong>' + row.worse_accu_config + ' ('  + worse_accu_yield + ')</div>'
        station3 = '<div style="color: #555;"><strong>Worse: </strong>' + row.bad_accu_config + ' ('  + bad_accu_yield + ')</div>'
        station4 = '<div style="color: #555;"><strong>Vary Bad: </strong>' + row.bad2_accu_config + ' ('  + bad2_accu_yield + ')</div>'
        station5 = '<div style="color: #555;"><strong>Bad: </strong>' + row.bad3_accu_config + ' ('  + bad3_accu_yield + ')</div>'
        
        return [datetime, station1, station2, station3, station4, station5]
      },
      trendLine: true,
      postUnits: '%',
      ymin: 'auto',
      ymax: 'auto',
      parseTime: false,
      barOpacity: 0.7,
      behaveLikeLine: true
    });
    var daily_graph = Morris.Bar({
      element: 'worst-daily-yield-data',
      smooth: true,
      continuousLine: true,
      behaveLikeLine: true,
