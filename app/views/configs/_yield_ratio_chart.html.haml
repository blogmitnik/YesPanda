      xkey: 'published_at',
      ykeys: ['worst_daily_yield', 'worse_daily_yield', 'bad_daily_yield', 'bad2_daily_yield', 'bad3_daily_yield'],
      labels: ['worst_daily_config', 'worse_daily_config', 'bad_daily_config', 'bad2_daily_config', 'bad3_daily_config'],
